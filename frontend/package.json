{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "rm -rf node_modules/.vite && wrangler types && concurrently -n client,worker -c green,cyan \"npm:dev:client\" \"npm:dev:worker\"",
		"dev:client": "VITE_DISABLE_CF_PLUGIN=1 vite",
		"dev:worker": "bash -c 'set -a && source .dev.vars && CLOUDFLARE_HYPERDRIVE_LOCAL_CONNECTION_STRING_HYPERDRIVE=$DATABASE_URL wrangler dev --port 18912 --config wrangler.dev.jsonc'",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build && wrangler deploy --config wrangler.jsonc",
		"cf-typegen": "wrangler types --config wrangler.jsonc"
	},
	"dependencies": {
		"@google-cloud/local-auth": "^3.0.1",
		"@xata.io/client": "^0.30.1",
		"postgres": "^3.4.4",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^7.9.4"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.13",
		"@eslint/js": "^9.33.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.8.1",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react-swc": "^4.0.0",
		"@vitest/coverage-v8": "^2.1.9",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.2.1",
		"eslint": "^9.33.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.18",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2",
		"vitest": "^2.1.9",
		"wrangler": "^4.43.0"
	}
}
